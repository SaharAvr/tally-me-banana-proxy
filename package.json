{
  "name": "tally-me-banana-proxy",
  "version": "4.1.0",
  "description": "A lightweight and powerful proxy server",
  "author": "SaharAvr <64190984+SaharAvr@users.noreply.github.com>",
  "license": "MIT",
  "repository": "https://github.com/SaharAvr/tally-me-banana-proxy.git",
  "keywords": [
    "proxy",
    "proxy-server",
    "http-proxy",
    "cors",
    "web-scraping",
    "express",
    "http",
    "https",
    "tcp",
    "socks",
    "socks5",
    "node"
  ],
  "main": "src/app.ts",
  "bin": {
    "tally": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev:http": "tsc-watch --onSuccess \"node dist/cli.js http 5050\"",
    "dev:tcp": "tsc-watch --onSuccess \"node dist/cli.js tcp 5050\"",
    "test": "jest --detectOpenHandles",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@applitools/eg-socks5-proxy-server": "0.5.4",
    "ssh2-custom": "1.3.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@types/node": "20.1.0",
    "@types/ssh2": "1.11.11",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "http-proxy": "1.18.1",
    "jest": "29.5.0",
    "socks-proxy-agent": "8.0.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "typescript": "5.0.4"
  }
}
